
<!-- ユーザーの詳細 -->
<div>
  <h2>User info</h2>
  <%= image_tag user.get_profile_image_url(100,100) %>

<table class="table table-striped">
  <tbody>
    <tr>
      <th>name</th>
      <th><%= user.name %></th>
    </tr>
    <tr>
      <th>introduction</th>
      <th><%= user.introduction %></th>
    </tr>
  </tbody>
</table>
<%= button_to 'Edit User', edit_user_path(current_user), method: :get, class: 'btn btn-primary' %>
</div>


<%= form_with model: book do |f| %>
<div>

  <h1>New book</h1>
  <h6>Title</h6>
  <%= f.text_field :title %>
  
  <h6>Option</h6>
  <%= f.text_area :body %>
  
  <br>
  <%= f.submit 'Create Book' %>

</div>

<% end %>